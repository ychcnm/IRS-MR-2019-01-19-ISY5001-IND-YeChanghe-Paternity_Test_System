package com.individual.paternity_test_system;

import java.lang.Number;

rule "Validation"
	dialect "mvel"
	ruleflow-group "ValidationRule"
	when
		ta : TestApplication( ( fa : father.isAlive == true && father.age <= ta.testee.age )|| ( ma : mother.isAlive == true && mother.age <= ta.testee.age ))
	then
		ErrorMessage er = new ErrorMessage();
		er.setErrorMessage( "You parents can't be more younger than you when they still alive! please correct your information" );
		insert( er );
		ta.setErrorMessage( er );
end
